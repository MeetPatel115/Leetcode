   "source": [
    "def mergesort(nums1,m,nums2,n):\n",
    "    \n",
    "    nums1=nums1[:m]\n",
    "    \n",
    "    nums2=nums2[:n]\n",
    "    \n",
    "    final= nums1+nums2\n",
    "    final.sort()\n",
    "    return final"
   ]
